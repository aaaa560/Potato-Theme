          "variable.parameter": {
            "color": "#B22222"
          },
          "function": {
            "color": "#EF0000",
            "font_weight": 700
          },
          "entity.name.function": {
            "color": "#8B0000",
            "font_weight": 800
          },
          "type": {
            "color": "#D9B44A"
          },
          "type.builtin": {
            "color": "#D9B44A",
            "font_weight": 700
          },
