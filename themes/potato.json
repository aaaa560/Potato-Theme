{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Potato Theme",
  "author": "Decaptado",
  "themes": [
    {
      "name": "Potato Theme",
      "appearance": "dark",
      "style": {
        "background": "#1E1E1E",
        "editor.background": "#1E1E1E",
        "editor.gutter.background": "#1E1E1E",
        "editor.active_line_number": "#C0C0C0",
        "editor.line_number": "#8A8A8A",
        "editor.active_line.background": "#2A2A2A",
        "info.background": "#1E1E1E",
        "info.foreground": "#D7D7D7",
        "border": "#444444",
        "border.variant": "#1E1E1E",
        "element.background": "#1E1E1E",
        "element.active": "#3A3A3A",
        "element.hover": "#2B2B2B",
        "status_bar.background": "#141414",
        "status_bar.foreground": "#D7D7D7",
        "title_bar.background": "#141414",
        "title_bar.foreground": "#D7D7D7",
        "tab.active_background": "#1E1E1E",
        "tab.inactive_background": "#292929",
        "tab_bar.background": "#1E1E1E",
        "panel.background": "#232323",
        "panel.focused_border": "#FF4500",
        "terminal.background": "#121212",
        "terminal.foreground": "#D7D7D7",
        "terminal.ansi.bright_yellow": "#FFD866",
        "terminal.ansi.yellow": "#FFB86C",
        "terminal.ansi.bright_blue": "#82AAFF",
        "terminal.ansi.blue": "#61AFEF",
        "terminal.ansi.bright_cyan": "#56B6C2",
