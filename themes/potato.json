        "terminal.ansi.bright_red": "#A02020",
        "terminal.ansi.red": "#8B0000",
        "terminal.ansi.white": "#D7D7D7",
        "terminal.ansi.black": "#1E1E1E",
        "scrollbar.thumb.background": "#4A4A4A",
        "scrollbar.thumb.hover_background": "#5E5E5E",
        "scrollbar.track.background": "#1E1E1E",
