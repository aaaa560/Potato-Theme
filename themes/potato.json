          "tag": {
            "color": "#7B91FF"
          },
          "exception": {
            "color": "#FF0000",
            "font_weight": 800
          },
          "meta.decorator": {
            "color": "#FFD866",
            "font_style": "italic",
            "font_weight": 700
          },
          "entity.name.class": {
            "color": "#FFD866",
            "font_weight": 800
          },
          "support.function.builtin": {
            "color": "#56B6C2"
          },
          "constant.language": {
            "color": "#D9B44A",
            "font_weight": 700
          }
        },
        "players": [
          {
            "cursor": "#FFD866",
            "selection": "#5B8DBF40"
          }
        ]
      }
    }
  ]
}
