          "comment": {
            "color": "#6B7280",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#FF0000",
            "font_weight": 700
          },
          "keyword.conditional": {
            "color": "#3F3F3F",
            "font_weight": 800
          },
          "keyword.control": {
            "color": "#3F3F3F",
            "font_weight": 800
          },
          "keyword.repeat": {
            "color": "#4B4B4B",
            "font_weight": 800
          },
          "keyword.function": {
            "color": "#8B0000",
            "font_weight": 700
          },
          "keyword.operator": {
            "color": "#56B6C2"
          },
          "string": {
            "color": "#00FF00"
          },
          "string.escape": {
            "color": "#FFD866",
            "font_weight": 700
          },
          "number": {
            "color": "#D9B44A"
          },
          "boolean": {
            "color": "#D9B44A",
            "font_weight": 700
          },
          "variable": {
            "color": "#B22222"
          },
          "variable.builtin": {
            "color": "#FFD866",
            "font_weight": 700
