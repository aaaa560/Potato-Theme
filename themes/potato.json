          "constant.builtin": {
            "color": "#B22222",
            "font_weight": 700
          },
          "operator": {
            "color": "#56B6C2"
          },
          "punctuation": {
            "color": "#D7D7D7"
          },
          "punctuation.bracket": {
            "color": "#D7D7D7"
          },
          "punctuation.delimiter": {
            "color": "#D7D7D7"
